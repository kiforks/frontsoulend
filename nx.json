{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"defaultProject": "platform",
	"defaultBase": "main",
	"namedInputs": {
		"default": ["{projectRoot}/**/*", "sharedGlobals"],
		"production": [
			"default",
			"!{projectRoot}/eslint.config.js",
			"!{projectRoot}/eslint.config.base.js",
			"!{projectRoot}/eslint.config.angular.js",
			"!{projectRoot}/.stylelint.json",
			"!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
			"!{projectRoot}/tsconfig.spec.json",
			"!{projectRoot}/jest.config.[jt]s",
			"!{projectRoot}/src/test-setup.[jt]s",
			"!{projectRoot}/test-setup.[jt]s",
			"!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
			"!{projectRoot}/.storybook/**/*",
			"!{projectRoot}/tsconfig.storybook.json"
		],
		"sharedGlobals": ["{workspaceRoot}/.github/workflows/ci.yml"]
	},
	"targetDefaults": {
		"@angular-devkit/build-angular:application": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["production", "^production"]
		},
		"@nx/eslint:lint": {
			"cache": true,
			"inputs": ["default", "{workspaceRoot}/.eslintrc.json", "{workspaceRoot}/eslint.config.js"]
		},
		"@nx/jest:jest": {
			"cache": true,
			"inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
			"options": {
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"nx-stylelint:lint": {
			"cache": true,
			"inputs": ["default", "{workspaceRoot}/.stylelint.js", "{workspaceRoot}/.stylelintignore"]
		},
		"e2e-ci--**/*": {
			"dependsOn": ["^build"]
		},
		"@nx/angular:package": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["production", "^production"]
		},
		"@nx/esbuild:esbuild": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["production", "^production"]
		},
		"build-storybook": {
			"cache": true,
			"inputs": ["default", "^production", "{projectRoot}/.storybook/**/*", "{projectRoot}/tsconfig.storybook.json"]
		}
	},
	"plugins": [
		{
			"plugin": "@nx/playwright/plugin",
			"options": {
				"targetName": "e2e"
			}
		},
		{
			"plugin": "@nx/eslint/plugin",
			"options": {
				"targetName": "lint"
			}
		},
		{
			"plugin": "nx-stylelint",
			"options": {
				"targetName": "stylelint"
			}
		}
	],
	"generators": {
		"@nx/angular:application": {
			"e2eTestRunner": "playwright",
			"linter": "eslint",
			"style": "scss",
			"unitTestRunner": "jest"
		},
		"@nx/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest",
			"publishable": true,
			"prefix": "fs",
			"tags": "scope:lib",
			"importPath": "@frontsoulend",
			"changeDetection": "OnPush",
			"style": "scss",
			"skipTests": true
		},
		"@schematics/angular:component": {
			"style": "scss",
			"changeDetection": "OnPush",
			"prefix": "app",
			"skipTests": true,
			"skipImport": true
		},
		"@schematics/angular:directive": {
			"prefix": "app",
			"skipTests": true,
			"skipImport": true,
			"flat": false
		},
		"@schematics/angular:pipe": {
			"skipTests": true,
			"skipImport": true,
			"flat": false
		},
		"@schematics/angular:service": {
			"skipTests": true,
			"flat": false
		},
		"@schematics/angular:guard": {
			"skipTests": true,
			"flat": false
		},
		"@schematics/angular:interceptor": {
			"skipTests": true,
			"flat": false
		},
		"@schematics/angular:interface": {
			"prefix": "app",
			"type": "interface"
		},
		"@schematics/angular:enum": {
			"type": "enum"
		},
		"@ngneat/spectator:spectator-component": {
			"style": "scss",
			"changeDetection": "OnPush",
			"prefix": "app",
			"skipImport": true
		},
		"@ngneat/spectator:spectator-directive": {
			"prefix": "app",
			"skipImport": true,
			"flat": false
		},
		"@ngneat/spectator:spectator-pipe": {
			"skipImport": true,
			"flat": false
		},
		"@ngneat/spectator:spectator-service": {
			"flat": false
		},
		"@nx/angular:component": {
			"style": "scss",
			"changeDetection": "OnPush",
			"prefix": "app",
			"skipTests": true,
			"skipImport": true
		},
		"@nx/angular:directive": {
			"prefix": "app",
			"skipTests": true,
			"skipImport": true,
			"flat": false
		},
		"@nx/angular:pipe": {
			"skipTests": true,
			"skipImport": true,
			"flat": false
		},
		"@nx/angular:service": {
			"skipTests": true,
			"flat": false
		},
		"@nx/angular:guard": {
			"skipTests": true,
			"flat": false
		},
		"@nx/angular:interceptor": {
			"skipTests": true,
			"flat": false
		},
		"@nx/angular:interface": {
			"prefix": "app",
			"type": "interface"
		},
		"@nx/angular:enum": {
			"type": "enum"
		},
		"@nx/angular:module": {
			"style": "scss",
			"skipImport": true,
			"flat": false
		}
	},
	"nxCloudId": "672be036b76a0e014484fa2b",
	"release": {
		"version": {
			"preVersionCommand": "npx nx run-many -t build"
		}
	}
}
