{
	"name": "frontsoulend",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"migrate:nx": "nx migrate latest && nx migrate --run-migrations --if-exists",
		"prepare": "husky",
		"format": "nx format:check",
		"format:fix": "nx format:write",
		"stylelint": "nx affected --target=stylelint --base=origin/main",
		"stylelint:fix": "nx affected --target=stylelint --fix --base=origin/main",
		"stylelint:all": "npx stylelint ./**/*.scss",
		"stylelint:all:fix": "npx stylelint ./**/*.scss --fix",
		"eslint": "nx affected --target=lint --base=origin/main",
		"eslint:fix": "nx affected --target=lint --fix --base=origin/main",
		"eslint:all": "npx eslint './**/*.{js,jsx,ts,tsx,html,json}'",
		"eslint:all:fix": "npx eslint './**/*.{js,jsx,ts,tsx,html,json}' --fix",
		"lint": "nx affected --target=lint,stylelint --base=origin/main && npm run format",
		"lint:fix": "nx affected --target=lint,stylelint --fix --base=origin/main && npm run format:fix",
		"lint:all": "npm run stylelint:all && npm run eslint:all && npm run format",
		"lint:all:fix": "npm run stylelint:all:fix && npm run eslint:all:fix && npm run format:fix",
		"test": "nx affected --target=test --base=origin/main",
		"test:coverage": "nx affected --target=test --base=origin/main --code-coverage",
		"test:ci": "nx affected --target=test --base=origin/main --code-coverage --ci",
		"prerelease": "nx affected --target=lint,stylelint,test --base=origin/main && npm run format",
		"build": "nx affected -t build --base=origin/main --configuration=development",
		"build:production": "nx affected -t build --base=origin/main --configuration=production",
		"build:ci:deploy-libs": "npx nx affected -t build --exclude='*,!tag:scope:lib' --base=HEAD~1 --head=HEAD --configuration=production",
		"packages:check": "ncu",
		"packages:update": "ncu -u",
		"packages:install": "ncu -u && npm install"
	},
	"private": true,
	"type": "module",
	"dependencies": {
		"@angular/animations": "19.0.5",
		"@angular/common": "19.0.5",
		"@angular/compiler": "19.0.5",
		"@angular/core": "19.0.5",
		"@angular/forms": "19.0.5",
		"@angular/platform-browser": "19.0.5",
		"@angular/platform-browser-dynamic": "19.0.5",
		"@angular/router": "19.0.5",
		"rxjs": "~7.8.1",
		"zone.js": "0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "19.0.6",
		"@angular-devkit/core": "19.0.6",
		"@angular-devkit/schematics": "19.0.6",
		"@angular/cdk": "19.0.4",
		"@angular/cli": "~19.0.0",
		"@angular/compiler-cli": "19.0.5",
		"@angular/language-service": "19.0.5",
		"@eslint/eslintrc": "^3.2.0",
		"@faker-js/faker": "^9.5.0",
		"@kiforks/eslint-config": "^1.2.5",
		"@kiforks/prettier-config": "^1.0.3",
		"@kiforks/stylelint-config": "^1.0.2",
		"@ngneat/spectator": "^19.4.1",
		"@nx/angular": "20.4.6",
		"@nx/devkit": "20.4.6",
		"@nx/esbuild": "20.4.6",
		"@nx/eslint": "20.4.6",
		"@nx/eslint-plugin": "20.4.6",
		"@nx/jest": "20.4.6",
		"@nx/js": "20.4.6",
		"@nx/playwright": "20.4.6",
		"@nx/storybook": "20.4.6",
		"@nx/web": "20.4.6",
		"@nx/workspace": "20.4.6",
		"@playwright/test": "^1.49.1",
		"@schematics/angular": "19.0.6",
		"@storybook/addon-a11y": "^8.6.0",
		"@storybook/addon-essentials": "^8.6.0",
		"@storybook/addon-interactions": "^8.6.0",
		"@storybook/addon-links": "^8.6.0",
		"@storybook/addon-queryparams": "^7.0.1",
		"@storybook/addon-storysource": "^8.6.0",
		"@storybook/angular": "^8.6.0",
		"@storybook/core-server": "^8.6.0",
		"@storybook/jest": "^0.2.3",
		"@storybook/test-runner": "^0.21.3",
		"@storybook/testing-library": "^0.2.2",
		"@swc-node/register": "~1.10.9",
		"@swc/core": "~1.11.1",
		"@swc/helpers": "~0.5.15",
		"@types/jest": "^29.5.14",
		"@types/node": "22.13.5",
		"autoprefixer": "^10.4.20",
		"esbuild": "^0.19.2",
		"eslint": "~9.21.0",
		"eslint-plugin-storybook": "^0.11.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"jest-preset-angular": "14.5.3",
		"jsonc-eslint-parser": "^2.4.0",
		"lint-staged": "^15.4.3",
		"ng-mocks": "^14.13.2",
		"ng-packagr": "19.2.0",
		"nx": "20.4.6",
		"nx-stylelint": "^18.0.0",
		"prettier": "^3.5.2",
		"storybook": "^8.6.0",
		"stylelint": "^16.14.1",
		"ts-jest": "^29.2.6",
		"ts-node": "10.9.2",
		"tslib": "^2.8.1",
		"typescript": "5.6.3"
	}
}
