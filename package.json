{
	"name": "frontsoulend",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"prepare": "husky",
		"format": "nx format:check",
		"format:fix": "nx format:write",
		"stylelint": "nx affected --target=stylelint --base=origin/main --head=HEAD",
		"stylelint:fix": "nx affected --target=stylelint --fix --base=origin/main --head=HEAD",
		"stylelint:all": "npx stylelint ./**/*.scss",
		"stylelint:fix-all": "npx stylelint ./**/*.scss --fix",
		"eslint": "nx affected --target=lint --base=origin/main --head=HEAD",
		"eslint:fix": "nx affected --target=lint --fix --base=origin/main --head=HEAD",
		"eslint:all": "npx eslint './**/*.{js,jsx,ts,tsx,html}'",
		"eslint:fix-all": "npx eslint './**/*.{js,jsx,ts,tsx,html}' --fix",
		"lint": "npm run format && npm run stylelint && npm run eslint",
		"lint:fix": "npm run format:fix && npm run stylelint:fix && npm run eslint:fix",
		"lint:all": "npm run format && npm run stylelint:all && npm run eslint:all",
		"lint:fix-all": "npm run format:fix && npm run stylelint:fix-all && npm run eslint:fix-all",
		"test": "nx affected --target=test --base=origin/main --head=HEAD",
		"test:coverage": "nx affected --target=test --base=origin/main --head=HEAD --code-coverage",
		"prerelease": "npm run lint && npm run test",
		"build": "nx affected -t build --base=origin/main --head=HEAD",
		"build:production": "nx affected -t build --base=origin/main --head=HEAD -configuration=production"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~18.2.0",
		"@angular/common": "~18.2.0",
		"@angular/compiler": "~18.2.0",
		"@angular/core": "~18.2.0",
		"@angular/forms": "~18.2.0",
		"@angular/platform-browser": "~18.2.0",
		"@angular/platform-browser-dynamic": "~18.2.0",
		"@angular/router": "~18.2.0",
		"rxjs": "~7.8.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~18.2.0",
		"@angular-devkit/core": "~18.2.0",
		"@angular-devkit/schematics": "~18.2.0",
		"@angular/cli": "~18.2.0",
		"@angular/compiler-cli": "~18.2.0",
		"@angular/language-service": "~18.2.0",
		"@eslint/js": "^9.8.0",
		"@ngneat/spectator": "^19.0.0",
		"@nx/angular": "20.0.0",
		"@nx/devkit": "20.0.0",
		"@nx/eslint": "20.0.0",
		"@nx/eslint-plugin": "20.0.0",
		"@nx/jest": "20.0.0",
		"@nx/js": "20.0.0",
		"@nx/playwright": "20.0.0",
		"@nx/web": "20.0.0",
		"@nx/workspace": "20.0.0",
		"@playwright/test": "^1.36.0",
		"@schematics/angular": "~18.2.0",
		"@swc-node/register": "~1.9.1",
		"@swc/core": "~1.5.7",
		"@swc/helpers": "~0.5.11",
		"@types/jest": "^29.5.12",
		"@types/node": "18.16.9",
		"@typescript-eslint/utils": "^8.0.0",
		"angular-eslint": "^18.3.0",
		"eslint": "~8.57.0",
		"eslint-config-kifor": "^1.1.7",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-playwright": "^1.6.2",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-preset-angular": "~14.1.0",
		"kifor-prettier-config": "^1.0.7",
		"kifor-stylelint-config": "^1.1.3",
		"lint-staged": "^15.2.10",
		"nx": "20.0.0",
		"nx-stylelint": "^17.1.5",
		"prettier": "^3.3.3",
		"stylelint": "^16.5.0",
		"ts-jest": "^29.1.0",
		"ts-node": "10.9.1",
		"tslib": "^2.3.0",
		"typescript": "~5.5.2",
		"typescript-eslint": "^8.0.0"
	}
}
