{
	"name": "frontsoulend",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"migrate:nx": "nx migrate latest && nx migrate --run-migrations --if-exists",
		"prepare": "husky",
		"format": "nx format:check",
		"format:fix": "nx format:write",
		"stylelint": "nx affected --target=stylelint --base=origin/main",
		"stylelint:fix": "nx affected --target=stylelint --fix --base=origin/main",
		"stylelint:all": "npx stylelint ./**/*.scss",
		"stylelint:all:fix": "npx stylelint ./**/*.scss --fix",
		"eslint": "nx affected --target=lint --base=origin/main",
		"eslint:fix": "nx affected --target=lint --fix --base=origin/main",
		"eslint:all": "npx eslint --flag unstable_config_lookup_from_file './**/*.{js,jsx,ts,tsx,html,json}'",
		"eslint:all:fix": "npx eslint --flag unstable_config_lookup_from_file './**/*.{js,jsx,ts,tsx,html,json}' --fix",
		"lint": "nx affected --target=lint,stylelint --base=origin/main && npm run format",
		"lint:fix": "nx affected --target=lint,stylelint --fix --base=origin/main && npm run format:fix",
		"lint:all": "npm run stylelint:all && npm run eslint:all && npm run format",
		"lint:all:fix": "npm run stylelint:all:fix && npm run eslint:all:fix && npm run format:fix",
		"test": "nx affected --target=test --base=origin/main",
		"test:coverage": "nx affected --target=test --base=origin/main --code-coverage",
		"test:ci": "nx affected --target=test --base=origin/main --code-coverage --ci",
		"prerelease": "nx affected --target=lint,stylelint,test --base=origin/main && npm run format",
		"extract-i18n": "nx affected -t extract-i18n --base=origin/main",
		"build": "nx affected -t build --base=origin/main --configuration=development",
		"build:production": "nx affected -t build --base=origin/main --configuration=production",
		"build:libs": "npx nx affected -t build --exclude='*,!tag:scope:lib' --base=origin/main --configuration=production",
		"build:libs:ci": "npx nx affected -t build --exclude='*,!tag:scope:lib' --base=HEAD~1 --head=HEAD --configuration=production",
		"packages:check": "ncu",
		"packages:update": "ncu -u",
		"packages:update:install": "ncu -u && npm install",
		"storybook": "nx storybook @frontsoulend/storybook",
		"storybook:composition": "nx run @frontsoulend/storybook:storybook-composition",
		"storybook:prod": "nx run @frontsoulend/storybook:build-storybook:prod"
	},
	"private": true,
	"type": "module",
	"dependencies": {
		"@angular/animations": "19.2.8",
		"@angular/common": "19.2.8",
		"@angular/compiler": "19.2.8",
		"@angular/core": "19.2.8",
		"@angular/forms": "19.2.8",
		"@angular/platform-browser": "19.2.8",
		"@angular/platform-browser-dynamic": "19.2.8",
		"@angular/router": "19.2.8",
		"rxjs": "~7.8.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "19.2.9",
		"@angular-devkit/core": "19.2.9",
		"@angular-devkit/schematics": "19.2.9",
		"@angular/cdk": "19.2.11",
		"@angular/cli": "~19.2.9",
		"@angular/compiler-cli": "19.2.8",
		"@angular/language-service": "19.2.8",
		"@angular/localize": "19.2.8",
		"@eslint/eslintrc": "^3.3.1",
		"@faker-js/faker": "^9.7.0",
		"@kiforks/eslint-config": "^1.3.1",
		"@kiforks/prettier-config": "^1.0.3",
		"@kiforks/stylelint-config": "^1.0.2",
		"@ngneat/spectator": "^19.4.1",
		"@nx/angular": "20.8.0",
		"@nx/devkit": "20.8.0",
		"@nx/esbuild": "20.8.0",
		"@nx/eslint": "20.8.0",
		"@nx/eslint-plugin": "20.8.0",
		"@nx/jest": "20.8.0",
		"@nx/js": "20.8.0",
		"@nx/playwright": "20.8.0",
		"@nx/storybook": "20.8.0",
		"@nx/web": "20.8.0",
		"@nx/workspace": "20.8.0",
		"@playwright/test": "^1.52.0",
		"@schematics/angular": "19.2.9",
		"@storybook/addon-a11y": "8.6.12",
		"@storybook/addon-essentials": "8.6.12",
		"@storybook/addon-interactions": "8.6.12",
		"@storybook/addon-links": "8.6.12",
		"@storybook/addon-queryparams": "^7.0.1",
		"@storybook/addon-storysource": "8.6.12",
		"@storybook/angular": "8.6.12",
		"@storybook/core-server": "8.6.12",
		"@storybook/jest": "^0.2.3",
		"@storybook/manager-api": "8.6.12",
		"@storybook/test-runner": "0.22.0",
		"@storybook/testing-library": "^0.2.2",
		"@storybook/theming": "8.6.12",
		"@swc-node/register": "~1.10.10",
		"@swc/core": "~1.11.22",
		"@swc/helpers": "~0.5.17",
		"@types/jest": "^29.5.14",
		"@types/node": "22.14.1",
		"autoprefixer": "^10.4.21",
		"chromatic": "^11.28.2",
		"esbuild": "^0.19.2",
		"eslint": "~9.25.1",
		"eslint-plugin-storybook": "^0.12.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"jest-preset-angular": "14.5.5",
		"jsonc-eslint-parser": "^2.4.0",
		"lint-staged": "^15.5.1",
		"ng-mocks": "^14.13.4",
		"ng-packagr": "19.2.2",
		"nx": "20.8.0",
		"nx-stylelint": "^18.0.0",
		"prettier": "^3.5.3",
		"storybook": "^8.6.12",
		"stylelint": "^16.19.0",
		"ts-jest": "^29.3.2",
		"ts-node": "10.9.2",
		"tslib": "^2.8.1",
		"typescript": "5.8.3"
	}
}
