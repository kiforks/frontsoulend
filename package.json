{
	"name": "frontsoulend",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"migrate": "nx migrate latest && nx migrate --run-migrations --if-exists",
		"prepare": "husky",
		"format": "nx format:check",
		"format:fix": "nx format:write",
		"stylelint": "nx affected --target=stylelint --base=origin/main",
		"stylelint:fix": "nx affected --target=stylelint --fix --base=origin/main",
		"stylelint:all": "npx stylelint ./**/*.scss",
		"stylelint:all:fix": "npx stylelint ./**/*.scss --fix",
		"eslint": "nx affected --target=lint --base=origin/main",
		"eslint:fix": "nx affected --target=lint --fix --base=origin/main",
		"eslint:all": "npx eslint './**/*.{js,jsx,ts,tsx,html}'",
		"eslint:all:fix": "npx eslint './**/*.{js,jsx,ts,tsx,html}' --fix",
		"lint": "nx affected --target=lint,stylelint --base=origin/main && npm run format",
		"lint:fix": "nx affected --target=lint,stylelint --fix --base=origin/main && npm run format:fix",
		"lint:all": "npm run stylelint:all && npm run eslint:all && npm run format",
		"lint:all:fix": "npm run stylelint:all:fix && npm run eslint:all:fix && npm run format:fix",
		"test": "nx affected --target=test --base=origin/main",
		"test:coverage": "nx affected --target=test --base=origin/main --code-coverage",
		"test:ci": "nx affected --target=test --base=origin/main --code-coverage --ci",
		"prerelease": "nx affected --target=lint,stylelint,test --code-coverage --base=origin/main && npm run format",
		"build": "nx affected -t build --base=origin/main --configuration=development",
		"build:production": "nx affected -t build --base=origin/main --configuration=production",
		"build:ci:deploy-libs": "npx nx affected -t build --exclude='*,!tag:scope:lib' --base=HEAD~1 --head=HEAD --configuration=production"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~18.2.0",
		"@angular/common": "~18.2.0",
		"@angular/compiler": "~18.2.0",
		"@angular/core": "~18.2.0",
		"@angular/forms": "~18.2.0",
		"@angular/platform-browser": "~18.2.0",
		"@angular/platform-browser-dynamic": "~18.2.0",
		"@angular/router": "~18.2.0",
		"rxjs": "~7.8.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~18.2.0",
		"@angular-devkit/core": "~18.2.0",
		"@angular-devkit/schematics": "~18.2.0",
		"@angular/cdk": "~18.2.0",
		"@angular/cli": "~18.2.0",
		"@angular/compiler-cli": "~18.2.0",
		"@angular/language-service": "~18.2.0",
		"@eslint/js": "^9.8.0",
		"@ngneat/spectator": "^19.0.0",
		"@nx/angular": "20.1.4",
		"@nx/devkit": "20.1.4",
		"@nx/esbuild": "20.1.4",
		"@nx/eslint": "20.1.4",
		"@nx/eslint-plugin": "20.1.4",
		"@nx/jest": "20.1.4",
		"@nx/js": "20.1.4",
		"@nx/playwright": "20.1.4",
		"@nx/web": "20.1.4",
		"@nx/workspace": "20.1.4",
		"@playwright/test": "^1.49.0",
		"@schematics/angular": "~18.2.0",
		"@swc-node/register": "~1.10.9",
		"@swc/core": "~1.10.0",
		"@swc/helpers": "~0.5.15",
		"@types/jest": "^29.5.14",
		"@types/node": "22.10.1",
		"@typescript-eslint/utils": "^8.17.0",
		"angular-eslint": "^18.2.0",
		"autoprefixer": "^10.4.20",
		"esbuild": "^0.19.2",
		"eslint": "~8.57.0",
		"eslint-config-kifor": "^1.1.7",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-playwright": "^2.1.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"jest-preset-angular": "~14.4.2",
		"kifor-prettier-config": "^1.0.7",
		"kifor-stylelint-config": "^2.0.0",
		"lint-staged": "^15.2.10",
		"ng-packagr": "~18.2.0",
		"nx": "20.1.4",
		"nx-stylelint": "^18.0.0",
		"prettier": "^3.4.2",
		"stylelint": "^16.11.0",
		"ts-jest": "^29.2.5",
		"ts-node": "10.9.2",
		"tslib": "^2.8.1",
		"typescript": "~5.5.4",
		"typescript-eslint": "^8.17.0"
	}
}
